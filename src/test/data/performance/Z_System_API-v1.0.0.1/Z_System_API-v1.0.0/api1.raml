#%RAML 0.8
title: Datetime format
mediaType: application/json
schemas:
  - example-schema: |
      {
        "type": "object",
        "$schema": "http://json-schema.org/draft-04/schema#",
        "description": "Test schema showing the issue",
        "properties": {
          "test1": {
              "type": "string",
              "description": "This property is marked as being required",
              "required": true
          },
          "test2": {
            "type" : "boolean"
          }
        }
      }

/res:
  get:
    responses:
      200:
        body:
          schema: example-schema
          example: |
            {
                "test1" : "asdasd",
                "test2" : true

            }
