#%RAML 1.0 Library
uses:

types:
  Element:
    properties:
      id?: string
      extension?: Extension[]

  Quantity:
    type: Element
    properties:
      value?: number
      comparator?:
        type: string
        enum: ['<', '<=', '>=', '>']
      unit?: string
      system?: string
      code?: string
  Age: Quantity
  Duration:
    type: Quantity
    description: a length of time
  ContactPoint:
    type: Element
    properties:
      system?:
        enum: [phone,fax,email,pager,other]
      value?:
      use?:
      rank?:
        type: number
      period?: Period
  Annotation:
    type: Element
    properties:
      /^author(Reference|String)$/?: Reference | string
      time?: string
      text?: string
  BackboneElement:
    type: Element
    properties:
      modifierExtension?: Extension
  Address:
    type: Element
    properties:
      use?:
        type: string
        enum: [home, work, temp, old]
      type?:
        type: string
        enum: [postal, physical, both]
      text?:
      line?: string[]
      city?:
      district?:
      state?:
      postalCode?:
      country?:
      period?: Period

  Period:
    type: Element
    properties:
      start?: string
      end?: string
  Reference:
    type: Element
    properties:
      reference?: string
      display?: string

  Identifier:
    properties:
      use?: string
      type?: CodeableConcept
      system?: string
      value?: string
      period?: Period
      assigner?: Reference
  Attachment:
    type: Element
    properties:
      contentType?:
        type: string
        description: Mime type of the content, with charset etc.
      language?:
        type: string
        description: Human language of the content (BCP-47)
      data?: string
      url?: string
      size?: number
      hash?: string
      title?: string
      creation?: string
  CodeableConcept:
    type: Element
    properties:
      coding?: Coding[]
      text?: string
  Coding:
    properties:
      system?: string
      version?: string
      code?: string
      display?: string
      userSelected?: boolean
  Signature:
    properties:
      type: Coding[]
      when: string
      /^who(Uri|Reference)$/: string | Reference
      contentType: string
      blob: string
  HumanName:
    type: Element
    properties:
      use?:
        type: string
        enum: [usual, official ,temp, nickname, anonymous, old, maiden]
      text?:
      family?: string[]
      given?: string[]
      prefix?: string[]
      suffix?: string[]
      period?: Period
  Narrative:
    properties:
      status: string
      div: string
  Range:
    type: Element
    properties:
      low?: integer
      high?: integer
  Ratio:
    type: Element
    properties:
      numerator?: Quantity
      denominator?: Quantity
  SampledData:
    description: A series of measurements taken by a device
    type: Element
    properties:
      origin: SimpleQuantity
      period: string
      factor?: number
      lowerLimit?: number
      upperLimit?: number
      dimensions: number
      data: string
  SimpleQuantity: Quantity
  Meta:
    type: Element
    properties:
      versionId?: string
      lastUpdated?: string
      profile?: string
      security?: Coding[]
      tag?: Coding[]
  Timing:
    type: Element
    properties:
      event?: string[]
      repeat?:
        type: Element
        properties:
          /^bounds(Duration|Range|Period)$/?: Duration | Range | Period
          count?: integer
          duration?: number
          durationMax?: number
          durationUnits?: string
          frequency?: integer
          frequencyMax?: integer
          period?: number
          periodMax?: number
          periodUnits?: string
          when?:
            type: string
            enum: [HS, WAKE, C, CM, CD, CV, AC, ACM, ACD, ACV, PC, PCM, PCD, PCV]
      code?: CodeableConcept
  Extension:
    properties:
      url: string

      /^value(Integer|Decimal|DateTime|Date|Instant|String|Uri|Boolean|Code|Markdown|Base64Binary|Coding|CodeableConcept|Attachment|Identifier|Quantity|Range|Period|Ratio|HumanName|Address|ContactPoint|Timing|Signature|Reference)$/: number | string | boolean | Coding | CodeableConcept | Attachment | Identifier | Quantity | Range | Period | Ratio | HumanName | Address | ContactPoint | Timing | Signature | Reference
